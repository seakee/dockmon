{
  "system": {
    "name": "dockmon",
    "run_mode": "debug",
    "http_port": ":8080",
    "read_timeout": 60,
    "write_timeout": 60,
    "version": "1.0.0",
    "debug_mode": true,
    "default_lang": "zh-CN",
    "jwt_secret": "",
    "token_expire": 604800
  },
  "log": {
    "driver": "stdout",
    "level": "debug",
    "path": "storage/logs/"
  },
  "databases": [
    {
      "enable": true,
      "db_type": "mysql",
      "db_name": "dockmon",
      "db_host": "db_host",
      "db_username": "db_username",
      "db_password": "db_password",
      "db_max_idle_conn": 10,
      "db_max_open_conn": 50,
      "db_max_lifetime": 3,
      "db_connect_retry_count": 5,
      "db_connect_retry_interval": 3
    },
    {
      "enable": false,
      "db_type": "mongo",
      "db_name": "db_name",
      "db_host": "db_host",
      "db_username": "db_username",
      "db_password": "db_password",
      "db_max_open_conn": 50
    }
  ],
  "cache": {
    "driver": "redis",
    "prefix": "dockmon"
  },
  "redis": [
    {
      "enable": true,
      "name": "dockmon",
      "host": "host",
      "auth": "",
      "max_idle": 30,
      "max_active": 100,
      "idle_timeout": 30,
      "prefix": "dockmon",
      "db": 0
    }
  ],
  "monitor": {
    "panic_robot": {
      "enable": false,
      "wechat": {
        "enable": false,
        "push_url": "https://qyapi.weixin.qq.com/cgi-bin/webhook/send?key=xxxx"
      },
      "feishu": {
        "enable": false,
        "push_url": "https://open.feishu.cn/open-apis/bot/v2/hook/xxxx"
      }
    }
  },
  "collector": {
    "monitor_self": true,
    "unstructured_log_line_flags": [
      "fatal error:",
      "[GIN-debug]",
      "[GIN-warning]",
      "panic:"
    ],
    "time_layout": [
      "2006-01-02",
      "2006-01-02 15:04:05",
      "2006-01-02 15:04:05.000",
      "2006-01-02T15:04:05Z07:00",
      "2006-01-02T15:04:05.000Z07:00",
      "2006-01-02T15:04:05.000-0700",
      "2006/01/02",
      "2006/01/02 15:04:05",
      "2006/01/02 15:04:05.000",
      "2006/01/02T15:04:05Z07:00",
      "2006/01/02T15:04:05.000Z07:00",
      "Mon Jan 2 15:04:05 MST 2006",
      "02 Jan 06 15:04 MST",
      "02 Jan 2006 15:04:05"
    ],
    "container_name": [
      "go-api"
    ]
  },
  "feishu" : {
    "enable": false,
    "app_id": "app_id",
    "app_secret": "app_secret",
    "encrypt_key": "",
    "group_webhook": "group_webhook"
  }
}
